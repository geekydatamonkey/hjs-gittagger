  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geekydatamonkey/hjs-gittagger/issues"
  },
  "homepage": "https://github.com/geekydatamonkey/hjs-gittagger#readme",
  "dependencies": {
    "react": "^0.13.3",
    "autoprefixer-core": "^5.2.1",
    "babel": "^5.8.21",
    "babel-core": "^5.8.21",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.15.6",
    "file-loader": "^0.8.4",
    "hjs-webpack": "^2.12.2",
    "json-loader": "^0.5.2",
    "postcss-loader": "^0.5.1",
    "react-hot-loader": "^1.2.8",
    "style-loader": "^0.12.3",
    "stylus-loader": "^1.2.1",
